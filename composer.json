{
	"name": "dmzx/emaillist",
	"type": "phpbb-extension",
	"description": "Allows a board forum owner to download and save a list of emails of users on the forum.",
	"homepage": "https://www.dmzx-web.net",
	"version": "1.0.3",
	"time": "2020-01-15",
	"keywords": [
		"phpbb",
		"extension",
		"emaillist"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "https://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Extension Lead Developer"
		},
		{
			"name": "Rich McGirr",
			"homepage": "https://www.phpbb.com/customise/db/author/rmcgirr83/contributions",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0",
		"phpbb/phpbb": ">=3.2.0,<4.4.0@dev"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Email List",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.0,<4.4.0@dev"
		},
		"version-check": {
			"host": "www.dmzx-web.net",
			"directory": "/versions",
			"filename": "emaillist_version.json",
			"ssl": true
		}
	}
}
